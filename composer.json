{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "ext-exif": "*",
        "ext-gd": "*",
        "codeat3/blade-forkawesome": "^1.10",
        "devdojo/app": "0.11.0",
        "devdojo/themes": "0.0.7",
        "faisal50x/blade-ionicons": "^1.0",
        "filament/filament": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "filament/tables": "^3.2",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "jaocero/radio-deck": "^1.2",
        "joshembling/image-optimizer": "^1.4",
        "lab404/laravel-impersonate": "^1.7",
        "lara-zeus/quantity": "^1.0",
        "laravel/framework": "^10.0",
        "laravel/passport": "^12.2",
        "laravel/socialite": "^5.14",
        "laravel/tinker": "^2.7",
        "livewire/livewire": "^3.4",
        "malzariey/filament-daterangepicker-filter": "^3.0",
        "mews/purifier": "^3.4",
        "nesbot/carbon": "^2.72",
        "outerweb/filament-image-library": "^2.6",
        "outerweb/image-library": "^2.5",
        "predis/predis": "^2.2",
        "ryangjchandler/blade-tabler-icons": "^2.3",
        "tcg/voyager": "1.7",
        "toin0u/geocoder-laravel": "^4.7",
        "tymon/jwt-auth": "@dev",
        "ysfkaya/filament-phone-input": "^3.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/dusk": "^7.8",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "6.4.0|^7.0",
        "pestphp/pest": "^2.8",
        "pestphp/pest-plugin-laravel": "^2.1",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Wave\\": "wave/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [],
            "providers": [
                "Wave\\WaveServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan storage:link",
            "@php artisan filament:upgrade"
        ]
    },
    "config": {
        "platform": {
            "php": "8.2.16"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "ralphjsmit/packages": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.ralphjsmit.com"
        },
        {
            "type": "composer",
            "url": "https://larapack.io"
        }
    ]
}
